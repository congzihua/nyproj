<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE sqlMapConfig PUBLIC "-//ibatis.apache.org//DTD SQL Map Config 2.0//EN"
    "http://ibatis.apache.org/dtd/sql-map-config-2.dtd">

<sqlMapConfig>

    <settings
        cacheModelsEnabled="true"
        enhancementEnabled="true"
        lazyLoadingEnabled="true"
        maxRequests="512"
        maxSessions="64"
        maxTransactions="50"
        useStatementNamespaces="false"
    />

    <transactionManager type="JDBC">
        <dataSource type="JNDI">
            <property name="DataSource" value="java:comp/env/jdbc/GwlzJNDIAlias"/>
        </dataSource>
    </transactionManager>

    <sqlMap resource="com/founder/enp/sqlmapdao/sql/sql-map-department.xml"/>
    <sqlMap resource="com/founder/enp/sqlmapdao/sql/sql-map-usergroup.xml"/>
    <sqlMap resource="com/founder/enp/sqlmapdao/sql/sql-map-user.xml"/>
    <sqlMap resource="com/founder/enp/sqlmapdao/sql/sql-map-template.xml"/>
    <sqlMap resource="com/founder/enp/sqlmapdao/sql/sql-map-doctypes.xml"/>
    <sqlMap resource="com/founder/enp/sqlmapdao/sql/sql-map-log.xml"/>
    <sqlMap resource="com/founder/enp/sqlmapdao/sql/sql-map-doc.xml"/>
    
    <sqlMap resource="com/roc/enp/sqlmapdao/sql/sql-map-flight.xml"/>
    <sqlMap resource="com/roc/enp/sqlmapdao/sql/sql-map-ticketsalloc.xml"/>
    <sqlMap resource="com/roc/enp/sqlmapdao/sql/sql-map-client.xml"/>
    <sqlMap resource="com/roc/enp/sqlmapdao/sql/sqlmap-flightinfo.xml"/>
    
     <sqlMap resource="com/roc/enp/sqlmapdao/sql/sql-map-unitsandup.xml"/>
    
</sqlMapConfig>
