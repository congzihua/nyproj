<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN"
    "http://ibatis.apache.org/dtd/sql-map-2.dtd">
<sqlMap namespace="dutytime">
	<select id="quyeryDutyTime" resultClass="com.roc.enp.entity.Dutytime">
    	select id,st,et from dutytime
    </select>
    <update id="editDutytime">
    	update dutytime set st=#st#,et=#et# where id=#id#
    </update>
</sqlMap>