      	<forward name="tipteam" path="/client/dsppages/tipteam.jsp" />
      	<forward name="teamUpDate" path="/client/dsppages/teamUpDate.jsp" />
      	<forward name="luggageInfo" path="/client/djppages/luggageInfo.jsp" />
      	<forward name="xxinfo" path="/client/djppages/xxinfo.jsp" />
      	<forward name="saltPointUnits" path="/client/dsppages/units/saltPointUnits.jsp"></forward>
