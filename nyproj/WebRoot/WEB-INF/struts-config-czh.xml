      	<forward name="fly_toFlyMain" path="/nyaddfold/clientfold/ajwindow/toFlyMain.jsp"></forward>
      	<forward name="fly_homePage" path="/nyaddfold/clientfold/ajwindow/homePage.jsp"></forward>
      	<forward name="fly_mainFramePage" path="/nyaddfold/clientfold/ajwindow/mainFramePage.jsp"></forward>
      	<forward name="fly_containPage" path="/nyaddfold/clientfold/ajwindow/containPage.jsp"></forward>
