<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.2//EN" "http://struts.apache.org/dtds/struts-config_1_2.dtd">

<struts-config>
  <data-sources />
  <form-beans>    
  </form-beans>
  <global-exceptions />
  <global-forwards />
  <action-mappings >      
       <action 
      path="/unitsDataAction" parameter="method"
      type="com.flyticket.system.action.UnitsDataAction"
      validate="false" scope="request" >
      	<forward name="mainPage" path="/client/zhzxpages/CCMainPage.jsp" />   
      	<forward name="exceptionDatas" path="/nyaddfold/dataop/exceptionDatas.jsp" />   	
      </action>
      
      <action 
      	path="/tfLoginAction"
      	type="com.tempflight.action.TfLoginAction"
      		validate="false" scope="request" >
	      	<forward name="success" path="/tempflight/saleticket/dspMain.jsp" redirect="true" />
	      	<forward name="djpMain" path="/tempflight/zhijipages/djpMain.jsp" redirect="true" />
		    <forward name="tip" path="/tflogin.jsp"/>
		    <forward name="admin" path="/tempflight/index.jsp"/>
		     <forward name="zhzxMain" path="/tempflight/zhihuizxinpages/zhzxMain.jsp"></forward>
	        <forward name="toSafeMain" path="/tempflight/safecheck/djpMain.jsp"></forward>
	        <forward name="toFlyMain" path="/tempflight/djsmpages/toFlyMain.jsp"></forward>
      </action>
      <action 
      path="/tfClientAction" parameter="method"
      type="com.tempflight.action.TfClientAction"
      validate="false" scope="request" >
      	<forward name="tempDspPage" path="/tempflight/saleticket/dspPage.jsp" />   
      	<forward name="tfHomePage" path="/tempflight/saleticket/homePage.jsp" /> 
      	<forward name="tfDspTop" path="/tempflight/saleticket/dspTop.jsp" />  
      	<forward name="tfDspBottom" path="/tempflight/saleticket/dspBottom.jsp" />  
      	<forward name="tempDpEdit" path="/tempflight/saleticket/dpEdit.jsp"/> 
      	<forward name="tip" path="/tempflight/saleticket/tip.jsp"/>
      	<forward name="luggageInfo" path="/tempflight/zhijipages/luggageInfo.jsp" />
      	<forward name="xxinfo" path="/tempflight/zhijipages/xxinfo.jsp" />
      	 <forward name="homePageDJP" path="/tempflight/safecheck/homePage.jsp" />
      	 <forward name="mainPage" path="/tempflight/safecheck/mainFramePage.jsp" /> 
      	 <forward name="containPage" path="/tempflight/safecheck/containPage.jsp" /> 
      	<forward name="fly_toFlyMain" path="/tempflight/djsmpages/toFlyMain.jsp"></forward>
      	<forward name="fly_homePage" path="/tempflight/djsmpages/homePage.jsp"></forward>
      	<forward name="fly_mainFramePage" path="/tempflight/djsmpages/mainFramePage.jsp"></forward>
      	<forward name="fly_containPage" path="/tempflight/djsmpages/containPage.jsp"></forward>
      	<forward name="tempFlightinfoList" path="/tempflight/saleticket/flightInfoList.jsp"></forward>
      </action>
      <action 
	      path="/tfDJPAction" parameter="method"
	      type="com.tempflight.action.TfDJPAction"
	      validate="false" scope="request" >
	      	<forward name="homePage" path="/tempflight/zhijipages/homePage.jsp" /> 
	      	<forward name="djpList" path="/tempflight/zhijipages/djpList.jsp" />
	      	<forward name="shchDJP" path="/tempflight/zhijipages/shchDJP.jsp" />    
	      	<forward name="shchDJPTEAM" path="/tempflight/zhijipages/shchDJPTEAM.jsp" />      	
      </action>
      <action 
	      path="/tfCommandCenter" parameter="method"
	      type="com.tempflight.action.TfCommandCenter"
	      validate="false" scope="request" >
	      	<forward name="mainPage" path="/tempflight/zhihuizxinpages/CCMainPage.jsp" />   
	      	<forward name="noFlightData" path="/tempflight/zhihuizxinpages/homePage.jsp" />
	      	<forward name="mainPageContain" path="/tempflight/zhihuizxinpages/CCContainPage.jsp" />
      </action>
 </action-mappings>

</struts-config>

